{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor0",
            "Effect": "Allow",
            "Action": [
                "cloudformation:List*",
                "cloudformation:Describe*",
                "cloudformation:Get*",
                "cloudformation:Validate*",
                "cloudformation:Detect*",
                "amplify:List*",
                "amplify:Get*",
                "s3:Get*",
                "s3:List*",
                "sqs:Receive*",
                "sqs:Send*",
                "sqs:DeleteMessage"
            ],
            "Resource": "*"
        },
        {
            "Sid": "VisualEditor1",
            "Effect": "Allow",
            "Action": "secretsmanager:GetSecretValue",
            "Resource": [
                "arn:aws:secretsmanager:REGION:ACCOUNT_ID:secret:amplify*",
                "arn:aws:secretsmanager:REGION:ACCOUNT_ID:secret:apple*"
            ]
        }
    ]
}